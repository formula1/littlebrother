{
  "env":{
    "browser": true
  },
  "ecmaFeatures": {
    "blockBindings": true
  },
  "rules": {
    "semi-spacing": [0],
    "no-console": [0],
    "quotes": [0],
    "curly": [0],
    "eol-last": [0],
    "new-cap": [0],
    "space-infix-ops": [0],
    "comma-spacing": [0],
    "no-underscore-dangle": [0],
    "camelcase": [0],
    "key-spacing": [0]
  }
}